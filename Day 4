import java.util.Scanner;
public class Sum_of_integers{
   public static void main(String[] args)
   {       
    int i,num=0,sum=0;
    {   
        System.out.println("Enter the 5 numbers : ");  
	}
		for (i=0;i<5;i++)
		{
		    Scanner sc = new Scanner(System.in);
		    num= sc.nextInt();
		    sum=sum+num;
	}
	 System.out.println("The sum of 5 no is : "+sum);
    }
}
